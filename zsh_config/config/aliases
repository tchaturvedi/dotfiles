alias reload="echo 'reloading zshrc' && source ~/.zshrc"
alias reloadplasma="killall plasmashell; plasmashell > /dev/null 2>&1 & disown"
alias psgrep="ps -ef | grep"
alias vim=nvim
alias pip=pip3

